@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Smart Parking - Monitoring Application</title>

    <link href="~/Content/bootstrap-yeti.min.css" rel="stylesheet" />
    <link href="~/Content/lealflet/leaflet.css" rel="stylesheet" />
